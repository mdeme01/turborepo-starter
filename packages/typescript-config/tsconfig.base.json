{
    "$schema": "https://json.schemastore.org/tsconfig",
    "display": "Base",
    "compilerOptions": {
        "target": "ES2022",
        "module": "NodeNext",
        "moduleResolution": "NodeNext",
        "moduleDetection": "force",
        "lib": ["ES2022"],
        "esModuleInterop": true,
        "skipLibCheck": true,
        "allowJs": true,
        "isolatedModules": true,
        "strict": true,
        "noImplicitAny": true
    }
}
